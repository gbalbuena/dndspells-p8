pico-8 cartridge // http://www.pico-8.com
version 18
__lua__

black = 0
white = 7
yellow = 10

txty = 10

function _init()
  ux = {
    selected = 1,
    modal = false };
end

function _update()
  input()
end

function _draw()
  cls(2)
  -- background()

  -- render list
  selected_spell = {}
  for index, spell in pairs(spells) do
    col = white
    if (ux.selected == index) then
      if (ux.modal) then
        selected_spell = spell
        col = 2
      else
        col = yellow
      end
    end
    spellY = 10 + (index * 10) - (ux.selected * 10);

    spr(get_spell_sprite(spell.school), 2, spellY)
    print(spell.name, 12, spellY + 1, col)
  end

  -- render modal
  if (ux.modal) then
    x = 6
    y = 6
    
    if (btnp(0)) txty = txty + y
    if (btnp(1)) txty = txty - y

    modal(selected_spell, x, txty)
  end

  if (not ux.modal) then
    header("paladin spells - 5th edition")
    footer()
  end
end

function get_spell_sprite(school)
  if (school == "abjuration") return 1
  if (school == "illusion") return 2
  if (school == "enchantment") return 3
  if (school == "divination") return 4
  if (school == "evocation") return 5
  if (school == "transmutation") return 6
  if (school == "necromancy") return 7
  if (school == "conjuration") return 8
end

function input()
  if (ux.selected >= 1) then
    if (btnp(2)) then 
      ux.selected = ux.selected - 1
    end
    if (btnp(3)) then
      ux.selected = ux.selected + 1
    end

    -- show/hide modal
    if (btnp(5)) then
      ux.modal = not ux.modal;
    end

    -- reset modal scroll
    if btnp(2) or btnp(3) or btnp(5) then
      txty = 6
    end
  end

  -- scroll limits
  if ux.selected == 0 then
    ux.selected = 1
  end
  if ux.selected == #spells + 1 then
    ux.selected = #spells
  end
end

function background()
  rectfill(0, 0, 128, 128, black)
end

function modal(spell, x, y)
  rectfill(0, 0, 128, 128, black)
  
  print(spell.name, x + 10, y, 8)
  spr(get_spell_sprite(spell.school), x, y - 2)

  print("level",      x, y + (6 * 2), 2)
  print(spell.level, 60, y + (6 * 2), 6)
  
  print("school",      x, y + (6 * 3), 2)
  print(spell.school, 60, y + (6 * 3), 6)
  
  print("casting time",      x, y + (6 * 4), 2)
  print(spell.casting_time, 60, y + (6 * 4), 6)

  print("range",      x, y + (6 * 5), 2)
  print(spell.range, 60, y + (6 * 5), 6)

  print("components",      x, y + (6 * 6), 2)
  print(spell.components, 60, y + (6 * 6), 6)

  print("duration",      x, y + (6 * 7), 2)
  print(spell.duration, 60, y + (6 * 7), 6)
  
  from = 1
  to = 28
  for i=0,(#spell.description / 28) do
    desc = sub(spell.description, from, to)
    print(desc, x, y + (6 * (8 + i + 1)), 6)
    from = from + 28
    to = to + 28
  end
end

function header(title)
  rectfill(0, 0, 128, 7, 2) -- top
  rectfill(0, 0, 128, 6, 1) -- top
  
  print(title, 1, 1, 8)
end

function footer()
  rectfill(0, 128, 128, 120, 2) -- bottom
  rectfill(0, 128, 128, 121, 1) -- bottom
  print("v1.0.0", 104, 122, 13)
end


